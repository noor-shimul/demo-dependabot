{
  "extends": ["config:base"],
  "enabledManagers": ["terraform"],
  "terraform": {
    "enabled": true
  },
  "packageRules": [
    {
      "managers": ["terraform"],
      "groupName": "All Terraform Providers",
      "matchPackagePatterns": [".*"],
      "enabled": true
    }
  ],
  "fileMatch": ["\\.tf$"],
  "includePaths": ["**/*.tf"],
  "prHourlyLimit": 20,
  "prConcurrentLimit": 5
}
