{
  "extends": [
    "config:base"
  ],
  "labels": ["renovate", "dependencies"],
  "packageRules": [
    {
      "managers": ["terraform"],
      "groupName": "terraform dependencies",
      "commitMessageTopic": "{{depName}} to {{newVersion}}",
      "commitMessageExtra": "Updated via Renovate"
    }
  ],
  "terraform": {
    "enabled": true
  }
}
